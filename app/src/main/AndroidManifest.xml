<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androiduitesting">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".dialog.DialogCaptureActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".gallery.CameraIntentActivity">
        </activity>
        <activity android:name=".gallery.GalleryActivity"></activity>
        <activity android:name=".movie.MovieActivity" />
        <activity android:name=".SecondaryActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>